<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite thread-count="1" verbose="1" name="BCNC">
    <listeners>
        <listener class-name="com.test.bcnc.TestListener"/>
    </listeners>

    <test name="prices" enabled="true" preserve-order="true">
        <groups>
            <run>
                <include name="priceAdapter"/>
            </run>
        </groups>
        <classes>
            <class name="com.test.bcnc.application.adapter.PriceSelectorAdapterTest"/>
        </classes>
    </test>
</suite>
